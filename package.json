        "svelte": "^4",
        "svelte-check": "^3",
        "svelte-preprocess": "^5",
        "ts-node": "^10.9.1",
        "tslib": "^2.5.0",
        "typescript": "^5",
        "vite": "^4"
    },
    "dependencies": {
        "ajv": "^8.12",
        "ajv-formats": "^2.1",
